<body>
    <div class="container">
        <h1 class="my-4">Inventory</h1>
        <table class="inventory-table table table-bordered">
            <thead>
                <tr>
                    <th>Photo</th>
                    <th>Name</th>
                    <th>Group Name</th>
                    <th>Subgroup Name</th>
                    <th>Quantity</th>
                    <th>Stock Date</th>
                    <th>Expiry Date</th>
                </tr>
            </thead>
            <tbody>
                @foreach (VBS_FrontEnd.Models.InventoryNames item in Model)
                {
                    <tr>
                        <td><img src="~/Images/@item.PhotoUrl" alt="@item.Name" /></td>
                        <td>@item.Name</td>
                        <td>@item.GroupName</td>
                        <td>@item.SubgroupName</td>
                        <td>@(item.Quantity + "g")</td>
                        <td style="text-align: center; font-weight: bold">@item.StockDate.ToShortDateString()</td>
                        @if (VBS_FrontEnd.Controllers.VBSController.DateChecker(DateTime.Now, item.ExpiryDate))
                        {
                            if (item.ExpiryDate < DateTime.Now)
                            {
                                <td style="background-color: rgba(247, 31, 31, 0.65); color: #FFFFFF; text-align:center; font-weight:bold">Expired</td>
                            }
                            else
                            {
                                <td style="background-color: rgba(244, 105, 0, 0.65); color: #FFFFFF; text-align: center; font-weight: bold">@(item.ExpiryDate.Day-DateTime.Now.Day) Day(s) left</td>
                            }
                        }
                        @if (!VBS_FrontEnd.Controllers.VBSController.DateChecker(DateTime.Now, item.ExpiryDate))
                        {
                            if (item.ExpiryDate < DateTime.Now)
                            {
                                <td style="background-color: rgba(247, 31, 31, 0.65); color: #FFFFFF; text-align: center; font-weight: bold">Expired</td>
                            }
                            else
                            {
                                <td style="background-color:rgba(115, 214, 115, 0.85); color: #FFFFFF; text-align: center; font-weight: bold">@item.ExpiryDate.ToShortDateString()</td>
                            }
                        }
                    </tr>
                }
            </tbody>
        </table>
    </div>
</body>
</html>
